!!!
%html
  %head
    %title= title

    = stylesheet_link_tag :all
    = javascript_include_tag :defaults
    = csrf_meta_tag

  %body
    = link_to 'Locations', locations_path

    .breadcrumbs
      = breadcrumbs
    .action_items
      = action_items

    #flash
      - flash.each do |key, value|
        .flash{:class => key}= value

    = yield
